<Page x:Class="NowaMasowka.Pages.Window1Pages.Page3_NewSource"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:NowaMasowka.Pages.Window1Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page3_NewSource">

    <Grid>
        <DataGrid x:Name="Window1Page3DataGrid" ItemsSource="{Binding}" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Style="{StaticResource DataGridStyle}">
            <DataGrid.Columns>
                <DataGridTextColumn Width="*" Header="Indeks" Binding="{Binding Indeks}"></DataGridTextColumn>
                <DataGridTextColumn Width="*" Header="Dostawca" Binding="{Binding Supplier}"></DataGridTextColumn>
                <DataGridTextColumn Width="*" Header="Zaklad" Binding="{Binding PlantNumber}"></DataGridTextColumn>
                <DataGridTextColumn Width="*" Header="DataOd" Binding="{Binding DateFrom}"></DataGridTextColumn>
                <DataGridTextColumn Width="*" Header="DataDo" Binding="{Binding DateTo}"></DataGridTextColumn>

            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Page>
