<Page x:Class="NowaMasowka.Pages.Window1Pages.Page4_DeleteSource"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:NowaMasowka.Pages.Window1Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page4_DeleteSource">

    <Grid>
        <DataGrid x:Name="Window1Page4DataGrid" ItemsSource="{Binding}" Style="{StaticResource DataGridStyle}">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Indeks"   Width="*"  Binding="{Binding Indeks}"></DataGridTextColumn>
                <DataGridTextColumn Header="Dostawca" Width="*"  Binding="{Binding Supplier}"></DataGridTextColumn>
                <DataGridTextColumn Header="Zaklad"   Width="*"  Binding="{Binding PlantNumber}"></DataGridTextColumn>
            </DataGrid.Columns>
        </DataGrid>

    </Grid>
</Page>
